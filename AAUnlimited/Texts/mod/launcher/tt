return function()

require "iuplua"
require "iupluacontrols"

frm = iup.frame {
	iup.hbox {
		iup.fill{},
		iup.button{title="AAA"},
		iup.fill{}
	}
}

frm.title = "heerrr"
frm.margin = "10x10"



vboxA = iup.vbox {
	iup.label {title="TABS AAA", expand="HORIZONTAL"},
	frame
}
vboxB = iup.vbox {
	iup.label { title="TABS BBB" },
	iup.button{title="BBB"}
}

vboxA.tabtitle = "Graphics settings"
vboxB.tabtitle = "Mods && data"

tabs = iup.tabs{vboxA, vboxB}

dlg = iup.dialog {
	iup.vbox {
		tabs,
		iup.button{title="Launch game", expand="HORIZONTAL", size="x32"}
	}
}

dlg:showxy(iup.CENTER, iup.CENTER)

if (iup.MainLoopLevel()==0) then
  iup.MainLoop()
end

end

